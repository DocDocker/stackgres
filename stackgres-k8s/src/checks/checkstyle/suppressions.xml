<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
  <suppress checks=".*" files=".*/target/.*" />
  <suppress checks=".*" files=".*/generated/.*" />

  <!-- Filters for checkstyle version mismatches -->
  <suppress message="Extra separation in import group before .*" checks="CustomImportOrder" files=".*" />
  <suppress message="Expected @throws tag for .*" checks="JavadocMethod" files=".*" />
</suppressions>
